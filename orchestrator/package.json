{
   "name": "compute-fabric-orchestrator",
   "version": "0.1.0",
   "description": "ComputeFabric Orchestrator - MVP service for job scheduling and management",
   "type": "commonjs",
   "main": "dist/index.js",
   "scripts": {
     "dev": "ts-node-dev --respawn src/index.ts",
     "build": "tsc",
     "start": "node dist/index.js",
     "db:push": "drizzle-kit push:pg",
     "db:generate": "drizzle-kit generate:pg",
     "db:migrate": "ts-node src/migrate.ts"
   },
   "dependencies": {
     "body-parser": "^1.20.2",
     "cors": "^2.8.5",
     "drizzle-orm": "^0.39.3",
     "express": "^4.18.2",
     "postgres": "^3.4.5",
     "stripe": "^13.3.0",
     "uuid": "^9.0.0",
     "dotenv": "^16.4.7"
   },
   "devDependencies": {
     "@types/body-parser": "^1.19.2",
     "@types/cors": "^2.8.13",
     "@types/express": "^4.17.17",
     "@types/node": "^20.3.1",
     "@types/pg": "^8.11.11",
     "@types/uuid": "^9.0.2",
     "drizzle-kit": "^0.30.4",
     "ts-node": "^10.9.1",
     "ts-node-dev": "^2.0.0",
     "typescript": "^4.9.5"
   },
   "author": "ComputeFabric Team",
   "license": "MIT"
 }